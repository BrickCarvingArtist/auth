*
	margin 0
	padding 0
	box-sizing border-box
	line-height 1
	font-family "Microsoft YaHei"
a
	color inherit
	text-decoration none
	cursor pointer
ul
ol
	list-style none
input[type="tel"]
input[type="password"]
textarea
select
button
	outline none
	color #333
	-webkit-appearance none
	background-color transparent
	border 1px solid transparent
textarea
	resize none
fieldset
	border-style solid
dialog
	border none