{
  "name": "auth",
  "version": "0.0.1",
  "description": "the auth center of https://*.ikindness.cn",
  "main": "app.js",
  "repository": "https://github.com/BrickCarvingArtist/auth.git",
  "scripts": {
    "start:dev": "export NODE_ENV=development && node server.js",
    "start:test": "export NODE_ENV=test && node server.js",
    "start:pro": "export NODE_ENV=production && node server.js",
    "build:dev": "yarn install",
    "build:test": "yarn install --production",
    "build:pro": "yarn install --production",
    "pregit:add": "yarn test",
    "git:add": "git add *",
    "eslint": "./node_modules/.bin/eslint ./",
    "test": "./node_modules/.bin/mocha"
  },
  "keywords": [
    "auth"
  ],
  "author": "BrickCarvingArtist",
  "license": "MIT",
  "babel": {
    "presets": [
      "env",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-runtime",
      "transform-decorators-legacy"
    ]
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.2",
    "classnames": "^2.2.5",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.4.3",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-helmet": "^3.2.0",
    "koa-ik-validity": "^0.2.2",
    "koa-logger": "^3.0.1",
    "koa-router": "^7.2.1",
    "mysql2": "^1.4.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "sequelize": "^4.13.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.8.0",
    "mocha": "^4.0.1"
  },
  "private": true
}
